{"ast":null,"code":"import { computed, ref, watch } from 'vue';\nexport default {\n  setup() {\n    const message = ref('Hello');\n    const name = ref('Marcin');\n\n    const reverseMessage = () => {\n      console.log('reverseMessage run');\n      return message.value.split('').reverse().join('');\n    };\n\n    const reverseMessageComputed = computed(() => {\n      console.log('reverseMessageComputed run');\n      return message.value.split('').reverse().join('');\n    });\n    setTimeout(() => {\n      console.log('name has been changed');\n      message.value = 'Goodbye';\n    }, 2000);\n    watch(message, (newValue, previousValue) => {\n      console.log('watcher:', newValue, previousValue);\n    });\n    return {\n      message,\n      reverseMessage,\n      name,\n      reverseMessageComputed\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAASA,QAAT,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAqC,KAArC;AAEA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,OAAM,GAAIH,GAAG,CAAC,OAAD,CAAnB;IACA,MAAMI,IAAG,GAAIJ,GAAG,CAAC,QAAD,CAAhB;;IACA,MAAMK,cAAa,GAAI,MAAM;MAC3BC,OAAO,CAACC,GAAR,CAAY,oBAAZ;MACA,OAAOJ,OAAO,CAACK,KAAR,CAAcC,KAAd,CAAoB,EAApB,EAAwBC,OAAxB,GAAkCC,IAAlC,CAAuC,EAAvC,CAAP;IACD,CAHD;;IAKA,MAAMC,sBAAqB,GAAIb,QAAQ,CAAC,MAAM;MAC5CO,OAAO,CAACC,GAAR,CAAY,4BAAZ;MACA,OAAOJ,OAAO,CAACK,KAAR,CAAcC,KAAd,CAAoB,EAApB,EAAwBC,OAAxB,GAAkCC,IAAlC,CAAuC,EAAvC,CAAP;IACD,CAHsC,CAAvC;IAKAE,UAAU,CAAC,MAAM;MACfP,OAAO,CAACC,GAAR,CAAY,uBAAZ;MACAJ,OAAO,CAACK,KAAR,GAAgB,SAAhB;IACD,CAHS,EAGP,IAHO,CAAV;IAKAP,KAAK,CAACE,OAAD,EAAU,CAACW,QAAD,EAAWC,aAAX,KAA6B;MAC1CT,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBO,QAAxB,EAAkCC,aAAlC;IACD,CAFI,CAAL;IAGA,OAAO;MAAEZ,OAAF;MAAWE,cAAX;MAA2BD,IAA3B;MAAiCQ;IAAjC,CAAP;EACD;;AAvBY,CAAf","names":["computed","ref","watch","setup","message","name","reverseMessage","console","log","value","split","reverse","join","reverseMessageComputed","setTimeout","newValue","previousValue"],"sourceRoot":"","sources":["/Users/marcintubisz/Projects/vue3.0-devtalk-app/src/lessons/lesson_04/ComputedLesson.vue"],"sourcesContent":["<template>\n  <h1>Lesson 4 - computed + watchers</h1>\n  <section>\n    <p>{{ reverseMessage() }}</p>\n    <p>{{ reverseMessageComputed }}</p>\n    <p>{{ name }}</p>\n  </section>\n</template>\n\n<script>\nimport { computed, ref, watch } from 'vue';\n\nexport default {\n  setup() {\n    const message = ref('Hello');\n    const name = ref('Marcin');\n    const reverseMessage = () => {\n      console.log('reverseMessage run');\n      return message.value.split('').reverse().join('');\n    };\n\n    const reverseMessageComputed = computed(() => {\n      console.log('reverseMessageComputed run');\n      return message.value.split('').reverse().join('');\n    });\n\n    setTimeout(() => {\n      console.log('name has been changed');\n      message.value = 'Goodbye';\n    }, 2000);\n\n    watch(message, (newValue, previousValue) => {\n      console.log('watcher:', newValue, previousValue);\n    });\n    return { message, reverseMessage, name, reverseMessageComputed };\n  },\n};\n</script>\n\n<style></style>\n<!--\n\n<template>\n  <h1>Lesson 4 - computed + watchers</h1>\n  <section>\n    <p>{{ message.split('').reverse().join('') }}</p>\n  </section>\n</template>\n\n<script>\nimport { ref } from 'vue';\n\nexport default {\n  setup() {\n    const message = ref('Hello');\n\n    return { message };\n  },\n};\n</script>\n\n<style></style>\n\n-->\n"]},"metadata":{},"sourceType":"module"}